


<%= simple_form_for [@experience, @booking] do |f| %>
  <%= f.input :date_start, placeholder:"Select Start Date..", as: :string, input_html: { data: { controller: "datepicker"} }%>
  <%= f.input :date_end, placeholder:"Select End Date..", as: :string, input_html: { data: { controller: "datepicker" } }  %>
  <%= f.submit %>
<% end %>
